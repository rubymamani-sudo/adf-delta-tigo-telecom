{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ucb-dw-tigo-222234"
		},
		"ls_sa_rw_tigo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sa_rw_tigo'"
		},
		"ls_sql_dw_tigo_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_dw_tigo'"
		},
		"ls_sql_dw_tigo_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-ucb-sisinfo-tigo-delta-234466.database.windows.net"
		},
		"ls_sql_dw_tigo_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dw_tigo"
		},
		"ls_sql_dw_tigo_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminruby"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ucb-dw-tigo-222234_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sa_rw_tigo_connectionString": {
						"value": "[parameters('ls_sa_rw_tigo_connectionString')]"
					},
					"ls_sql_dw_tigo_password": {
						"value": "[parameters('ls_sql_dw_tigo_password')]"
					},
					"ls_sql_dw_tigo_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_dw_tigo_properties_typeProperties_server')]"
					},
					"ls_sql_dw_tigo_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_dw_tigo_properties_typeProperties_database')]"
					},
					"ls_sql_dw_tigo_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_dw_tigo_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}