						"          duracion as integer",
						"     ),",
						"     errors: true) ~> cast1",
						"cast1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          tipo as string,",
						"          numero_origen as string,",
						"          numero_destino as string,",
						"          fecha as date,",
						"          hora as timestamp,",
						"          duracion as integer",
						"     ),",
